<table style="width:100%">
  <tr>
    <th> <%= @artist_name %>'s product </th>
    <th> Other identical product </th>
  </tr>
  <%- @artist_matches.each do |match| -%>
  <tr>
    <td> <%= link_to (match['name_1'] + " - $#{'%.02f' % match['price_1']}"), match['prod_url_1'] %> <br> </td>
    <td> <%= link_to (match['name_2'] + " - $#{'%.02f' % match['price_2']}"), match['prod_url_2'] %>
      <%= link_to (", By #{@id_to_artist[match['artist_id_2']]}"), ("/artist/#{match['artist_id_2']}")%><br> </td>
  </tr>
  <%- end -%>
</table>
